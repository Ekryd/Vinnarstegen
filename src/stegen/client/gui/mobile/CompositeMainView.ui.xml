<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:score="urn:import:stegen.client.gui.score"
	xmlns:message="urn:import:stegen.client.gui.message"
	xmlns:gameresult="urn:import:stegen.client.gui.playeraction"
	xmlns:rules="urn:import:stegen.client.gui.rules">
	
	<ui:style>
	.rulesInsets {
		padding-left: 15px;
		padding-top: 10px;
	}
		
	</ui:style>
	
	  <g:StackLayoutPanel ui:field="mainContentTable"  animationDuration="1000" >
		  <g:stack>
		    <g:customHeader size="30"> <g:Label text="Poängställning"/> </g:customHeader>
		    <g:ScrollPanel>
		    	<score:ListScorePanel ui:field="scorePanel"/>
		    </g:ScrollPanel>
		  </g:stack>
		  <g:stack>
		    <g:customHeader size="30"> <g:Label text="Meddelanden"/> </g:customHeader>
		    <g:ScrollPanel>
		    	<message:MessagePanel ui:field="messagePanel"/>
		    </g:ScrollPanel>
		  </g:stack>
		  <g:stack>
		    <g:customHeader size="30"> <g:Label text="Matcher"/> </g:customHeader>
		    <g:ScrollPanel>
		    	<gameresult:GameResultPanel ui:field="gameResultPanel"/>
		    </g:ScrollPanel>
		  </g:stack>
		  <g:stack>
		    <g:customHeader size="30"> <g:Label text="Inloggning"/> </g:customHeader>
		    <g:ScrollPanel>
		    	<gameresult:LoginStatusTable ui:field="loginStatusTable"/>
		    </g:ScrollPanel>
		  </g:stack>
		  <g:stack>
		    <g:customHeader size="30"> <g:Label text="Övrigt"/> </g:customHeader>
		    <g:ScrollPanel>
		    	<gameresult:PlayerMiscCommandTable ui:field="playerMiscCommandTable"/>
		    </g:ScrollPanel>
		  </g:stack>
		  <g:stack>
		    <g:customHeader size="30"> <g:Label text="Regler"/> </g:customHeader>
		    <g:ScrollPanel>
		    <rules:RulesPanel  ui:field="rulesPanel"/>
		    </g:ScrollPanel>
		  </g:stack>
	 </g:StackLayoutPanel>
	 
	  
</ui:UiBinder> 